{
  "name": "@ai-utils/promptpress",
  "author": "themataleao@gmail.com",
  "description": "Lightweight prompt template library for AI models",
  "version": "0.0.2",
  "licence": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
